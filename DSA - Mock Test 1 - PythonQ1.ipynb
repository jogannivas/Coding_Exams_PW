    "def moving(arr):\n",
    "    n = len(arr)\n",
    "    left = 0  \n",
    "\n",
    "    \n",
    "    for i in range(n):\n",
    "        if arr[i] != 0:\n",
    "            arr[left] = arr[i]\n",
    "            left += 1\n",
    "\n",
    "\n",
    "    while left < n:\n",
    "        arr[left] = 00\n",
    "        left += 1\n",
    "\n",
    "    return arr\n"
   ]
